var reviews = [];
var currentReviewsCount = 6; 

document.getElementById('back-to-start').addEventListener('click', function(event) {
  event.preventDefault();
  currentReviewsCount = 0; 
  document.getElementById('load-more').click(); 
  document.getElementById('load-more').style.display = 'inline';
});


document.getElementById('load-more').addEventListener('click', function(event) {
  event.preventDefault();
  var serverId = 436034;
  var reviewsContainer = document.getElementById('reviews');
  reviews = []; 

  fetch('https://minecraft-server-list.com/server/fetch_more_reviews.php', {
    method: 'POST',
    headers: {
      'Content-Type': 'application/x-www-form-urlencoded'
    },
    body: 'server_id=' + serverId + '&offset=' + currentReviewsCount
  })
  .then(function(response) {
    return response.json();
  })
  .then(function(reviewsData) {
    reviews = reviewsData; 
    reviewsContainer.innerHTML = ''; 

    reviews.forEach(function(review) {
      var reviewElement = document.createElement('div');
      reviewElement.className = 'review';

      reviewElement.innerHTML = `
        <div class="review-header">
          <span class="review-name">${review.voter_name}</span>
          <span class="review-date">${review.review_date}</span>
        </div>
        <div class="review-rating">
          ${[1, 2, 3, 4, 5].map(function(i) {
            return `<span class="star ${i <= review.rating ? 'filled' : ''}">&#9733;</span>`;
          }).join('')}
        </div>
        <p class="review-text">${review.review_text}</p>
      `;

      reviewsContainer.appendChild(reviewElement);
    });

    currentReviewsCount += reviewsData.length;

   if (reviewsData.length < 6) {
  document.getElementById('load-more').style.display = 'none';
  document.getElementById('back-to-start').style.display = 'inline';
} else {
  document.getElementById('back-to-start').style.display = 'none';
}
  });
});


