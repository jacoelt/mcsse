# Cache expiry time in seconds. 0 means no expiry, cache will be valid indefinitely.
CACHE_EXPIRY=0

# If True, the search engine will only use cached data and not query the websites. Non cached data will not be retrieved.
USE_CACHE_ONLY=False

# Logging configuration
LOG_LEVEL=INFO